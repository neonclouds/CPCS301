#include <iostream>
using namespace std;

int main()
{
	int n = 0;
	cout << "How many integers to reverse?";
	cin >> n;
	int* reverseArray = new int[n];


	for (int i = 0; i < n; i++)
	{
		cin >> reverseArray[i];
	}
	for (int i = n - 1; i != -1; i--)
	{
		cout << reverseArray[i];
	}

	system("PAUSE");
}
